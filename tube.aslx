    <object name="Central">
      <code>central</code>
      <up>eastbound</up>
      <down>westbound</down>
      <facts type="scriptdictionary">
        <item key="106"><![CDATA[
          msg ("We're passing through an{if game.abandonedstationcount>0:other} abandoned station here, called British Museum. There's not very much left of it now, just some assorted railway junk where the platforms used to be. Most people never even know it's there.")
          game.abandonedstationcount = game.abandonedstationcount + 1
        ]]></item>
      </facts>
    </object>
